{
  "name": "photo-migration-system",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "A comprehensive photo album migration system supporting multiple social platforms",
  "keywords": [
    "photo",
    "migration",
    "social media",
    "google photos",
    "react"
  ],
  "author": "Your Name",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "vite",
    "repl:dev": "vite --host 0.0.0.0 --port ${PORT:-3000}",
    "repl:build": "vite build && vite preview --host 0.0.0.0 --port ${PORT:-3000}",
    "repl:health": "node replit-health-check.js",
    "repl:setup": "npm install && npm run repl:health",
    "repl:clean": "rm -rf node_modules package-lock.json && npm install",
    "repl:restart": "pkill -f vite || true && npm run repl:dev",
    "codespaces:dev": "vite --host 0.0.0.0 --port 3000",
    "codespaces:build": "vite build && vite preview --host 0.0.0.0 --port 3000",
    "codespaces:setup": "npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ui": "jest --watch --verbose"
  },
  "dependencies": {
    "@react-oauth/google": "^0.12.2",
    "axios": "^1.10.0",
    "bottleneck": "^2.19.5",
    "bull": "^4.16.5",
    "flickr-sdk": "^7.0.0-beta.9",
    "framer-motion": "^11.18.2",
    "googleapis": "^150.0.1",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.2",
    "react-toastify": "^11.0.5",
    "recharts": "^3.0.0",
    "redis": "^5.5.6",
    "sharp": "^0.34.2",
    "socket.io-client": "^4.8.1",
    "web-streams-polyfill": "^4.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "postcss": "^8.4.27",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.5"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
